	WHEN f$InPosition AND listColdCall3BetIP Call Force
	WHEN NOT f$InPosition AND list4BetOOP RaiseTo f$StandardRaise Force
	WHEN NOT f$InPosition AND listColdCall3BetOOP Call Force
	
